@using BitirmeProjesi.Models.Sınıflar
@model List<AYTDeneme>
@{
    ViewBag.Title = "AYTDenemeListele";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<title>AYT Deneme Sonuçları</title>
<div style="background-color: #E9ECEF; border-radius: 5px; margin-top: 30px; margin-left:25px; margin-right:25px ">
    <p style="text-align: center; font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans- serif; font-size: 20px; color: #8789FF;margin-top:14px; ">
        AYT Deneme Sonuçları
    </p>
</div>
<div style="margin-top:5px; margin-right:25px;margin-left:25px;">
    <table class="table table-bordered" style="margin-top:30px; text-align:center;">
        <tr>
            <th>ID</th>
            <th>Türkçe Net</th>
            <th>Sosyal Net</th>
            <th>Matematik Net</th>
            <th>Fen Net</th>
            <th>Toplam Net</th>
            <th>Puan</th>
            <th>Yayın</th>
            <th>Tarih</th>
            <th>Düzenle</th>
        </tr>
        @foreach (var x in Model)
        {
            <tr>
                <td>@x.ID</td>
                <td>@x.EdebNet</td>
                <td>@x.SosNet</td>
                <td>@x.MatNet</td>
                <td>@x.FenNet</td>
                <td>@x.TopNet</td>
                <td>@x.Puan</td>
                <td>@x.Yayın</td>
                <td>@x.Tarih</td>
                <td>
                    <div style="width:150px;">
                        <a href="/Deneme/AYTDenemeTasi/@x.ID" class="btn btn-warning">
                            <img src="~/Görseller/update.png" />
                        </a>
                        <a href="/Deneme/AYTDenemeSil/@x.ID" class="btn btn-danger">
                            <img src="~/Görseller/Delete.png" />
                        </a>
                    </div>
                </td>
            </tr>
        }
    </table>
    <a class="btn btn-primary" href="/Deneme/AYTDenemeEkle/" style="margin-left:500px;">Yeni Deneme Sonucu Ekle</a>
</div>


